const pjson = require('../package.json')
const version = pjson.version
const fs = require('fs')
const path = require('path')

const js = `
const version = {
  number: '${version}'
}

export default version
`

fs.writeFileSync(
  path.join(__dirname, '../src/footer/footerVersion.js'),
  js,
  'utf-8'
)
